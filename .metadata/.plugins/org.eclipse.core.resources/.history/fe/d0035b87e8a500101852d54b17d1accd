package operators.relationalOperator;

public class RelationalOperatorMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1 =10;
		int num2 = 15;
		
		System.out.println("The Given "+ num1 +" > " + num2 +" is : " +(num1 > num2) );
		System.out.println("The Givem "+ num1 + " < " + num2 + " is : " +(num1 < num2));
		
		int var1 = 10;
		int var2 = 10;
		System.out.println("The Given "+ var1 +" >= " + var2 + " is : "+ (var1 >= var2) );
		System.out.println("The Given "+ var1 +" <= " + var2 + " is : "+ (var1 <= var2) );
		
		char charValue1= 'a';
		char charvalue2= 'b';
		System.out.println("The Given Character "+ charValue1 + " == " + charvalue2 +" is : "+ (charValue1 == charvalue2));
		charvalue2 = 'a';
		System.out.println("The Given Character "+ charValue1 + " == " + charvalue2 +" is : "+ (charValue1 == charvalue2));
	}

}
