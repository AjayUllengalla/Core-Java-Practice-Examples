package practice.programs;

public class SwapOfTwoNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1 = 30;
		int num2 = 40;
		
		num1 = num1 + num2;
		num1 = num2 - num1;
		num2 = num1 - num2;
		System.out.println(num1);
		System.out.println(num2);

	}

}
