package practice.programs;

public class EvenOrOdd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 15;
		
		if(num / 2 == 0) {
			System.out.println("The Given Number is Even");
		} else {
			System.out.println("The Given Number is Odd");
		}
		
		num = 8;
		if(num / 2 == 0) {
			System.out.println("The Given Number is Even");
		} else {
			System.out.println("The Given Number is Odd");
		}
	}

}
