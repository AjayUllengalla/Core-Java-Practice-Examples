package operators.relationalOperator;

public class RelationalOperatorMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1 =10;
		int num2 = 15;
		
		System.out.println("The Given "+ num1 +" > " + num2 +" is : " +(num1 > num2) );
		System.out.println("The Givem "+ num1 + " > " + num2 + " is : " +(num1 < num2));

	}

}
