package operators.unaryOperator;

public class UnaryOperatorMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int var1 = 30;
		int var2 = 40;
		
		var1 = var1++;
		System.out.println(var1);
		
		var1 = ++var1;
		System.out.println(var1);
		
		var1 = ++var1 + var2++;
		System.out.println(var1);

	}

}
